RewriteEngine on

# Przekierowanie domeny

RewriteCond %{SERVER_PORT} !^443$ [OR]
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]
RewriteCond %{HTTP_HOST} !^www\.nietylkodj\.pl$ [NC] 
RewriteRule ^(.*)$ https://www.nietylkodj\.pl/$1 [R=301,L
ErrorDocument 403 /zletrafiles
ErrorDocument 500 /zletrafiles